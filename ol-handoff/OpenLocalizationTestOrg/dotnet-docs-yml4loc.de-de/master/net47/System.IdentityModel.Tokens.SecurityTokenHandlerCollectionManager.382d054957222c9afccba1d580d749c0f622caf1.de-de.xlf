      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>A class that manages multiple, named security token handler collections.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.IdentityModel.Tokens.SecurityTokenHandlerCollectionManager"&gt;&lt;/xref&gt;</ph> class for the specified named service.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The name of the associated service.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">&lt;xref href="System.IdentityModel.Tokens.SecurityTokenHandlerCollectionManager.ServiceName"&gt;&lt;/xref&gt;</ph> property is set to this name.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Determines whether this instance contains a security token handler collection for the specified usage.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The usage for which to find a token handler collection.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`true`</ph> if this instance contains a token handler collection for the specified usage; otherwise, <ph id="ph2">`false`</ph>.</source>
        </trans-unit>
