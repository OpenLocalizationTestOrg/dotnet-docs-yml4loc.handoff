        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">&lt;xref href="System.DirectoryServices.Protocols.SearchOption"&gt;&lt;/xref&gt;</ph> enumeration specifies the search options which define how the search will behave.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Prevents referrals from being generated when the search results are returned.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The value is equal to 1.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Instructs the server to search all naming contexts that are subordinate to the search base.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The value is equal to 2.</source>
        </trans-unit>
