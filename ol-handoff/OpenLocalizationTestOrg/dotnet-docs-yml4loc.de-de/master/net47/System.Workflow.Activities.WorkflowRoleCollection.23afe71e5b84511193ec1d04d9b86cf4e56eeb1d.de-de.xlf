        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents a collection of <ph id="ph1">&lt;xref href="System.Workflow.Activities.WorkflowRole"&gt;&lt;/xref&gt;</ph> objects.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>This class cannot be inherited.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source><ph id="ph1">&lt;xref:System.Workflow.Activities.WorkflowRoleCollection&gt;</ph> provides programmatic access to check whether an identity associated with a <ph id="ph2">&lt;xref:System.Workflow.Activities.WorkflowRole&gt;</ph> has been added to a <ph id="ph3">&lt;xref:System.Workflow.Activities.HandleExternalEventActivity&gt;</ph> or <ph id="ph4">&lt;xref:System.Workflow.Activities.WebServiceInputActivity&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.Workflow.Activities.WorkflowRoleCollection"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Returns a value indicating whether the <ph id="ph1">&lt;xref href="System.Workflow.Activities.WorkflowRoleCollection"&gt;&lt;/xref&gt;</ph> contains the specified identity.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>A string that contains the identity for which to search.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`true`</ph> if the <ph id="ph2">&lt;xref href="System.Workflow.Activities.WorkflowRoleCollection"&gt;&lt;/xref&gt;</ph> contains the identity; otherwise <ph id="ph3">`false`</ph>.</source>
