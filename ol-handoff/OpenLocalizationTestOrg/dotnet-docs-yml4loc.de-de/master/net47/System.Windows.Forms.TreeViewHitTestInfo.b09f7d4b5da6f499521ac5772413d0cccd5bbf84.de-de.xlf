      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Contains information about an area of a <ph id="ph1">&lt;xref href="System.Windows.Forms.TreeView"&gt;&lt;/xref&gt;</ph> control or a <ph id="ph2">&lt;xref href="System.Windows.Forms.TreeNode"&gt;&lt;/xref&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.TreeViewHitTestInfo&gt;</ph> is returned by the <ph id="ph2">&lt;xref:System.Windows.Forms.TreeView&gt;</ph> class when the <ph id="ph3">&lt;xref:System.Windows.Forms.TreeView.HitTest%2A&gt;</ph> method is called.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.Windows.Forms.TreeViewHitTestInfo"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
