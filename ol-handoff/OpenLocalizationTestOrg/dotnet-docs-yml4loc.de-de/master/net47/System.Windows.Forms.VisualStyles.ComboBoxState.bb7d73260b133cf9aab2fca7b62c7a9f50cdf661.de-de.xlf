      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the visual state of a combo box that is drawn with visual styles.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ComboBoxState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ComboBox.DropDownButton&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ComboBoxState&gt;</ph> values are also used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.ComboBoxRenderer.DrawDropDownButton%2A?displayProperty=fullName&gt;</ph> and <ph id="ph3">&lt;xref:System.Windows.Forms.ComboBoxRenderer.DrawTextBox%2A?displayProperty=fullName&gt;</ph> methods.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The combo box is disabled.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The combo box is hot.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The combo box has the default appearance.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The combo box is pressed.</source>
