        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Determines which date to include in the names of the <ph id="ph1">&lt;xref href="Microsoft.VisualBasic.Logging.FileLogTraceListener"&gt;&lt;/xref&gt;</ph> class log files.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>Assign one of the <ph id="ph1">&lt;xref:Microsoft.VisualBasic.Logging.LogFileCreationScheduleOption&gt;</ph> enumeration values to the <ph id="ph2">&lt;xref:Microsoft.VisualBasic.Logging.FileLogTraceListener.LogFileCreationSchedule%2A&gt;</ph> property to specify which date to include in the names of its log files.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Include the current date in the log file name.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Do not include the date in the log file name.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Include the first day of the current week in the log file name.</source>
