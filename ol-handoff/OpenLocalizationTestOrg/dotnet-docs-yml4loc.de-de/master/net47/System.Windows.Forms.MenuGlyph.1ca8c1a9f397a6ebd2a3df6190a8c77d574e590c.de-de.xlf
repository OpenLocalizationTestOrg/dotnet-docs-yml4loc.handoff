        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>These values represent the different types of symbols that can be drawn on a menu item.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Draws a submenu arrow.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Draws a menu bullet.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Draws a menu check mark.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The maximum value available by this enumeration (equal to the <ph id="ph1">&lt;xref href="System.Windows.Forms.MenuGlyph.Bullet"&gt;&lt;/xref&gt;</ph> value).</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
