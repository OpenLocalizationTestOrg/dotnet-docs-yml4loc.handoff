        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source><ph id="ph1">&lt;xref:System.CodeDom.CodePropertyReferenceExpression&gt;</ph> can be used to represent a reference to the value of a property.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.CodeDom.CodePropertyReferenceExpression.TargetObject%2A&gt;</ph> property specifies the object that contains the property to reference.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.CodeDom.CodePropertyReferenceExpression.PropertyName%2A&gt;</ph> property specifies the name of the property to reference.</source>
