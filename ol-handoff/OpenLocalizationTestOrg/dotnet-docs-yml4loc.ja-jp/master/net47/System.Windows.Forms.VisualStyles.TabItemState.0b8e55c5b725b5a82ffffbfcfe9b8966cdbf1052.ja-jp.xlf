        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the visual state of a tab item that is drawn with visual styles.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the following classes:</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.TabItemState&gt;</ph> values are also used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.TabRenderer.DrawTabItem%2A?displayProperty=fullName&gt;</ph> method.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The tab item is disabled.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The tab item is hot.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The tab item is normal.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The tab item is selected.</source>
