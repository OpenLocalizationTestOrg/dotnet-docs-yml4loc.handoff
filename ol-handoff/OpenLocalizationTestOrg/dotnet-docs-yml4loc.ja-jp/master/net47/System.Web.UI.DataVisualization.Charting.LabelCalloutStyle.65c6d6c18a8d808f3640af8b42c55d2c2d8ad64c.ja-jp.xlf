      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the type of leader line that should be drawn from the point to the label.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.LabelCalloutStyle&gt;</ph> enumeration represents the type of leader line that should be drawn from the point to the label.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>It is used in conjunction with the <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.SmartLabelStyle.CalloutStyle%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.SmartLabelStyle&gt;</ph> object.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Line will terminate to a box drawn around the label text.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Line will terminate at the label without any added style.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Line will terminate with a horizontal line beneath the label text.</source>
