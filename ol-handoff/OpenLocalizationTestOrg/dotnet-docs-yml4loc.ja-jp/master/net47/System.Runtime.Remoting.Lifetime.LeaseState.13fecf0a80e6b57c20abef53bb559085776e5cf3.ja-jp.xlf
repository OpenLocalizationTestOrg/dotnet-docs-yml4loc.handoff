        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Indicates the possible lease states of a lifetime lease.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Runtime.Remoting.Lifetime.ILease.CurrentState%2A&gt;</ph> property returns the lease status for a lease.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The lease is active and has not expired.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The lease has expired and cannot be renewed.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The lease has been created, but is not yet active.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The lease is not initialized.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The lease has expired and is seeking sponsorship.</source>
