      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the type of change that occurred on the <ph id="ph1">&lt;xref href="System.IO.Ports.SerialPort"&gt;&lt;/xref&gt;</ph> object.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>This enumeration is used with the <ph id="ph1">&lt;xref:System.IO.Ports.SerialPort.PinChanged&gt;</ph> event.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
