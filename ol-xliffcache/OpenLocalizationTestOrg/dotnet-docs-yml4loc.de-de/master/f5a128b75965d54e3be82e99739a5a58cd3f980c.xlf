      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents the presence information of a peer.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The presence of a peer specifies a certain level of participation in a peer collaboration network, such as whether or not the peer has logged into or out of the peer collaboration network, or has set presence status to "Away".</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Initializes a new default instance of a <ph id="ph1">&lt;xref href="System.Net.PeerToPeer.Collaboration.PeerPresenceInfo"&gt;&lt;/xref&gt;</ph> object.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>New instances of <ph id="ph1">&lt;xref:System.Net.PeerToPeer.Collaboration.PeerPresenceStatus&gt;</ph> generated by this constructor contains a default PeerPresenceInfo value equal to "Online".</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Initializes an instance of a <ph id="ph1">&lt;xref href="System.Net.PeerToPeer.Collaboration.PeerPresenceInfo"&gt;&lt;/xref&gt;</ph> object that includes a Unicode string describing the presence status the local peer.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT">
          <source>New instances of <ph id="ph1">&lt;xref:System.Net.PeerToPeer.Collaboration.PeerPresenceStatus&gt;</ph> generated by this constructor contain a default PeerPresenceInfo value equal to "Online".</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Status of the Peer.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Description of the presence state.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Gets or sets a Unicode string further describing the presence status for the local peer.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT">
          <source>This string is limited to 255 Unicode characters.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Unicode string describing the presence status of a peer.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
