        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the reason a thread is waiting.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The thread wait reason is only valid when the <ph id="ph1">&lt;xref:System.Diagnostics.ThreadState&gt;</ph> is <ph id="ph2">&lt;xref:System.Diagnostics.ThreadState.Wait&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The thread is waiting for event pair high.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The thread is waiting for event pair low.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Thread execution is delayed.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The thread is waiting for the scheduler.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The thread is waiting for a free virtual memory page.</source>
        </trans-unit>
