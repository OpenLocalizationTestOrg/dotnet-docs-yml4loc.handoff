      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Defines a <ph id="ph1">&lt;xref href="System.IdentityModel.Tokens.SecurityTokenHandler"&gt;&lt;/xref&gt;</ph> that processes Windows Username tokens.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.IdentityModel.Tokens.WindowsUserNameSecurityTokenHandler&gt;</ph> class extends the <ph id="ph2">&lt;xref:System.IdentityModel.Tokens.UserNameSecurityTokenHandler&gt;</ph> class to add validation of tokens.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.IdentityModel.Tokens.WindowsUserNameSecurityTokenHandler"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Gets a value that indicates whether this handler supports validation of tokens of type <ph id="ph1">&lt;xref href="System.IdentityModel.Tokens.UserNameSecurityToken"&gt;&lt;/xref&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><ph id="ph1">`true`</ph> if the handler supports validation of tokens of type <ph id="ph2">&lt;xref href="System.IdentityModel.Tokens.UserNameSecurityToken"&gt;&lt;/xref&gt;</ph>; otherwise, <ph id="ph3">`false`</ph>.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Always <ph id="ph1">`true`</ph>.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Validates the specified token and returns its claims.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The token to validate.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
