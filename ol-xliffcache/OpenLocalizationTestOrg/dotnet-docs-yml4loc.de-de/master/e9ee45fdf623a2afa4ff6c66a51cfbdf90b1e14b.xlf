        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the state of an item that is being drawn.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>This enumeration is used by members such as <ph id="ph1">&lt;xref:System.Windows.Forms.DrawItemEventArgs.State%2A?displayProperty=fullName&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The item is checked.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Only menu controls use this value.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
