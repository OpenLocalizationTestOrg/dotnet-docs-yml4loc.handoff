        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">&lt;xref href="System.DirectoryServices.ActiveDirectory.ReplicationOperationInformation"&gt;&lt;/xref&gt;</ph> class contains information about an Active Directory Domain Services replication operation.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.DirectoryServices.ActiveDirectory.ReplicationOperationInformation"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Gets the current replication operation.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>A <ph id="ph1">&lt;xref href="System.DirectoryServices.ActiveDirectory.ReplicationOperation"&gt;&lt;/xref&gt;</ph> object that represents the current replication operation.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Gets the time that this replication operation started.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>A <ph id="ph1">&lt;xref href="System.DateTime"&gt;&lt;/xref&gt;</ph> object that contains the date and time that this replication operation started.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
