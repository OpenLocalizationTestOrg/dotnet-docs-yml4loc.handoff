        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents the resource key for the <ph id="ph1">&lt;xref href="System.Windows.DataTemplate"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <ph id="ph1">&lt;xref href="System.Windows.DataTemplateKey"&gt;&lt;/xref&gt;</ph> class.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
