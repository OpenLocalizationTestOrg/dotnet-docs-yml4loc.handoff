        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Enables a non-control component to emulate the data-binding behavior of a Windows Forms control.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>A <ph id="ph1">&lt;xref:System.Windows.Forms.Binding&gt;</ph> contains a reference to its <ph id="ph2">&lt;xref:System.Windows.Forms.IBindableComponent&gt;</ph> owner through the <ph id="ph3">&lt;xref:System.Windows.Forms.Binding.BindableComponent%2A&gt;</ph> property.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.Control&gt;</ph> class implements this interface, so if your bindable component inherits from <ph id="ph2">&lt;xref:System.Windows.Forms.Control&gt;</ph>, you do not need to implement this interface.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Gets or sets the collection of currency managers for the <ph id="ph1">&lt;xref href="System.Windows.Forms.IBindableComponent"&gt;&lt;/xref&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The collection of <ph id="ph1">&lt;xref href="System.Windows.Forms.BindingManagerBase"&gt;&lt;/xref&gt;</ph> objects for this <ph id="ph2">&lt;xref href="System.Windows.Forms.IBindableComponent"&gt;&lt;/xref&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Gets the collection of data-binding objects for this <ph id="ph1">&lt;xref href="System.Windows.Forms.IBindableComponent"&gt;&lt;/xref&gt;</ph>.</source>
