        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents an enumeration that specifies the kind of the collection that is applied to the properties in the conceptual schema definition language (CSDL) files.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>You can apply this to the properties of any type.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The collection has bag semantics.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The items are unordered and duplicates are acceptable.</source>
        </trans-unit>
