      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Enumerates values for the speaking rate of prompts.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>Members of the <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> enumeration are used by the <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle.%23ctor%2A&gt;</ph> constructor, by the <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptBuilder.AppendText%2A&gt;</ph> method, and by the <ph id="ph4">&lt;xref:System.Speech.Synthesis.PromptBuilder.StartStyle%2A&gt;</ph> method to specify the rate of speech for spoken text.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptStyle.Rate%2A&gt;</ph> property gets the rate for a <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle&gt;</ph> object using a <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptRate&gt;</ph> instance.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Indicates an extra-fast rate.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Indicates an extra-slow rate.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Indicates a fast rate.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Indicates a medium rate.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Indicates no rate is specified.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Indicates a slow rate.</source>
