      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Defines identifiers that indicate the persistence scope of a tab in the Properties window.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source><ph id="ph1">&lt;xref:System.ComponentModel.PropertyTabScope&gt;</ph> indicates the persistence scope of a tab that is displayed in the Properties window when a component of a design document has an associated <ph id="ph2">&lt;xref:System.ComponentModel.PropertyTabAttribute&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>This tab is specific to the current component.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>This tab is added to the Properties window for the current component only and is removed when the component is no longer selected.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>This tab is specific to the current document.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>This tab is added to the Properties window and is removed when the currently selected document changes.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>This tab is added to the Properties window and can only be removed explicitly by a parent component.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>This tab is added to the Properties window and cannot be removed.</source>
