        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the location of tick marks in a <ph id="ph1">&lt;xref href="System.Windows.Forms.TrackBar"&gt;&lt;/xref&gt;</ph> control.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>Use the members of this enumeration to set the value of the <ph id="ph1">&lt;xref:System.Windows.Forms.TrackBar.TickStyle%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.Windows.Forms.TrackBar&gt;</ph> control.</source>
        </trans-unit>
