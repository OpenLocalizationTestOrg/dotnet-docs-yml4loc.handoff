        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT">
          <source>128</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT">
          <source>128</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Creates a cryptographic object to perform the <ph id="ph1">&lt;xref href="System.Security.Cryptography.Rijndael"&gt;&lt;/xref&gt;</ph> algorithm.</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" extradata="MT">
          <source>The default implementation of <ph id="ph1">&lt;xref:System.Security.Cryptography.Rijndael&gt;</ph> is <ph id="ph2">&lt;xref:System.Security.Cryptography.RijndaelManaged&gt;</ph>.</source>
        </trans-unit>
