    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Describes how a child element is vertically positioned or stretched within a parent's layout slot.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source><ph id="ph1">&lt;xref:System.Windows.FrameworkElement.Height%2A&gt;</ph> and <ph id="ph2">&lt;xref:System.Windows.FrameworkElement.Width%2A&gt;</ph> properties that are explicitly set on an element take precedence over the <ph id="ph3">&lt;xref:System.Windows.HorizontalAlignment.Stretch&gt;</ph> property value.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The child element is aligned to the bottom of the parent's layout slot.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The child element is aligned to the center of the parent's layout slot.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The child element stretches to fill the parent's layout slot.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The child element is aligned to the top of the parent's layout slot.</source>
        </trans-unit>
