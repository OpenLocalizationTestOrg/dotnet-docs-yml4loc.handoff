      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Contains information about changes made to the activity associated with the designer.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>When changes are made to an activity associated with a designer, the <ph id="ph1">&lt;xref:System.Workflow.ComponentModel.Design.ActivityChangedEventArgs&gt;</ph> will contain information about the change.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Initializes an instance of the <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Design.ActivityChangedEventArgs"&gt;&lt;/xref&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The Activity associated with the designer that has changed.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The member of the Activity that has changed.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The value of the member before the change.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The value of the member after the change.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Gets the <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Activity"&gt;&lt;/xref&gt;</ph> that has changed.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT">
          <source>Gets the Activity that has changed in the designer.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Activity"&gt;&lt;/xref&gt;</ph> that has changed.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>The member of the <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Activity"&gt;&lt;/xref&gt;</ph> being changed.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.ComponentModel.MemberDescriptor&gt;</ph> contains information about the member of the Activity that has changed.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>A <ph id="ph1">&lt;xref href="System.ComponentModel.MemberDescriptor"&gt;&lt;/xref&gt;</ph> that describes the member that has changed.</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>The new value of the member of the <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Activity"&gt;&lt;/xref&gt;</ph> that has changed.</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT">
          <source>NewValue is an object that contains information about the new value.</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>The new value of the member that has changed.</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>The old value of the member of the <ph id="ph1">&lt;xref href="System.Workflow.ComponentModel.Activity"&gt;&lt;/xref&gt;</ph> that has changed.</source>
        </trans-unit>
