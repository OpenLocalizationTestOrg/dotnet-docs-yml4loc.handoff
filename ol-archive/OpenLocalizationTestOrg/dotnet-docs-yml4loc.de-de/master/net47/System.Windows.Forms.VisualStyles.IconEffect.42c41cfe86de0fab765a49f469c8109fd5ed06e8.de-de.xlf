        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the visual effect that the visual style will apply to an icon.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.IconEffect&gt;</ph> values represent the return values of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.GetEnumValue%2A?displayProperty=fullName&gt;</ph> method when it is called with an argument value of <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.EnumProperty?displayProperty=fullName&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The icon is alpha-blended.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The icon glows.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>No visual effect is applied to the icon.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>The icon pulses.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The icon has a shadow.</source>
