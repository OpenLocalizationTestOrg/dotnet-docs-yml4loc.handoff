    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies a style for markers.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.MarkerStyle&gt;</ph> enumeration represents the style used for a series or data point marker.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>It is used as a value for the <ph id="ph1">&lt;xref:System.Web.UI.DataVisualization.Charting.LegendItem.MarkerStyle%2A&gt;</ph> property and the <ph id="ph2">&lt;xref:System.Web.UI.DataVisualization.Charting.DataPointCustomProperties.MarkerStyle%2A&gt;</ph> property.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>A circular marker is displayed.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>A cross-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>A diamond-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>No marker is displayed for the series or data point.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>A square marker is displayed.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>A 10-point star-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>A 4-point star-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>A 5-point star-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>A 6-point star-shaped marker is displayed.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>A triangular marker is displayed.</source>
        </trans-unit>
