        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Use <ph id="ph1">&lt;xref href="System.Runtime.InteropServices.ComTypes.EXCEPINFO"&gt;&lt;/xref&gt;</ph> instead.</source>
