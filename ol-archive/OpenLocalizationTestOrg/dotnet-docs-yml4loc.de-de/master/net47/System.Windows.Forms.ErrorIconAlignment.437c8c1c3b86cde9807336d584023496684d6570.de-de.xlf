        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies constants indicating the locations that an error icon can appear in relation to the control with an error.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>This enumeration is used by <ph id="ph1">&lt;xref:System.Windows.Forms.ErrorProvider&gt;</ph>.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The icon appears aligned with the bottom of the control and the left of the control.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>The icon appears aligned with the bottom of the control and the right of the control.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The icon appears aligned with the middle of the control and the left of the control.</source>
        </trans-unit>
